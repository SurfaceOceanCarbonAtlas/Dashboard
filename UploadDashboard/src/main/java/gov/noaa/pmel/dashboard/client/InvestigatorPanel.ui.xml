<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my='urn:import:gov.noaa.pmel.dashboard.client'>

    <ui:style src="Investigator.css"/>

    <g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="firstValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="middleValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="lastValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="idValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="idTypeValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="orgValue"/>
        </g:FlowPanel>
    </g:FlowPanel>

</ui:UiBinder>
