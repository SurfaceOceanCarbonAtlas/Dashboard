<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:my="urn:import:gov.noaa.pmel.dashboard.client.metadata">

    <ui:style src="../EditSocatMetadata.css"/>


    <g:ScrollPanel>
        <g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="columnNameValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="fullNameValue"/>
            </g:FlowPanel>
            <my:LabeledListBox addStyleNames="{style.areapanel}" ui:field="varTypeList"/>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="unitValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="accuracyValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="precisionValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="missingValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="flagNameValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="sampleLocValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="sampleElevValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="observeTypeValue"/>
                <my:LabeledListBox addStyleNames="{style.cellpanel}" ui:field="measureMethodValue"/>
            </g:FlowPanel>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="methodDescValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="methodRefsValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="manipDescValue"/>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="storageMethodValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="durationValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="analysisTempValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="replicationValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="researcherValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="instrumentsValue"/>
            </g:FlowPanel>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="addnInfoValue"/>
        </g:FlowPanel>
    </g:ScrollPanel>

</ui:UiBinder>
