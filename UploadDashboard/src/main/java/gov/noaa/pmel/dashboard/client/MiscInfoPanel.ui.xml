<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my='urn:import:gov.noaa.pmel.dashboard.client'>

    <ui:style src="MiscInfo.css"/>

    <g:ScrollPanel addStyleNames="{style.scrollpanel}">
        <g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="idValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="nameValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="sectionValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="doiValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="accessValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="citationValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="websiteValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="downloadValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="fundAgencyValue"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="{style.rowpanel}">
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="fundIdValue"/>
                <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="fundTitleValue"/>
            </g:FlowPanel>
            <my:LabeledTextBox addStyleNames="{style.areapanel}" ui:field="projectValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="synopsisValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="purposeValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="refsValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="portsValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="addnInfoValue"/>
            <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="historyValue"/>
        </g:FlowPanel>
    </g:ScrollPanel>

</ui:UiBinder>
