<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="SidebarPanel.css"/>

    <g:DockLayoutPanel unit="EM">
        <g:west size="5">
            <g:ScrollPanel addStyleNames="{style.sidescroll}">
                <g:Button addStyleNames="{style.modbutton}" ui:field="addButton"/>
                <g:Button addStyleNames="{style.modbutton}" ui:field="removeButton"/>
                <g:ListBox addStyleNames="{style.valslist}" visibleItemCount="10" ui:field="valsList"/>
            </g:ScrollPanel>
        </g:west>
        <g:center>
            <g:ScrollPanel addStyleNames="{style.mainscroll}" ui:field="mainPanel"/>
        </g:center>
    </g:DockLayoutPanel>

</ui:UiBinder>
