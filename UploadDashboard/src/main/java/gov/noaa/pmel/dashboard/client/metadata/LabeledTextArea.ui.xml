<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:style src="LabeledText.css"/>

    <g:FlowPanel>
        <g:HTML addStyleNames="{style.areaprefix}" ui:field="prefixHtml"/>
        <g:TextArea addStyleNames="{style.areavalue}" ui:field="valueArea"/>
    </g:FlowPanel>

</ui:UiBinder>
