<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my='urn:import:gov.noaa.pmel.dashboard.client'>

    <ui:style src="Platform.css"/>

    <g:FlowPanel>
        <my:LabeledTextBox addStyleNames="{style.areapanel}" ui:field="idValue"/>
        <my:LabeledTextBox addStyleNames="{style.areapanel}" ui:field="nameValue"/>
        <my:LabeledListBox addStyleNames="{style.areapanel}" ui:field="typeList"/>
        <my:LabeledTextBox addStyleNames="{style.areapanel}" ui:field="ownerValue"/>
        <my:LabeledTextBox addStyleNames="{style.areapanel}" ui:field="countryValue"/>
    </g:FlowPanel>

</ui:UiBinder>
