<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="Platform.css"/>

    <g:FlowPanel>
        <g:Grid addStyleNames="{style.grid}">
            <g:row>
                <g:customCell>
                    <g:HTML addStyleNames="{style.gridlabel}" ui:field="idHtml"/>
                </g:customCell>
                <g:customCell>
                    <g:TextBox addStyleNames="{style.gridtext}" ui:field="idValue"/>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML addStyleNames="{style.gridlabel}" ui:field="nameHtml"/>
                </g:customCell>
                <g:customCell>
                    <g:TextBox addStyleNames="{style.gridtext}" ui:field="nameValue"/>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML addStyleNames="{style.gridlabel}" ui:field="typeHtml"/>
                </g:customCell>
                <g:customCell>
                    <g:ListBox visibleItemCount="1" ui:field="typeList"/>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML addStyleNames="{style.gridlabel}" ui:field="ownerHtml"/>
                </g:customCell>
                <g:customCell>
                    <g:TextBox addStyleNames="{style.gridtext}" ui:field="ownerValue"/>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML addStyleNames="{style.gridlabel}" ui:field="countryHtml"/>
                </g:customCell>
                <g:customCell>
                    <g:TextBox addStyleNames="{style.gridtext}" ui:field="countryValue"/>
                </g:customCell>
            </g:row>
        </g:Grid>
    </g:FlowPanel>

</ui:UiBinder>
