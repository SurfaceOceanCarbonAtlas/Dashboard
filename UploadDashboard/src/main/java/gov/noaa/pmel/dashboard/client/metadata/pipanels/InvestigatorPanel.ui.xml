<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my="urn:import:gov.noaa.pmel.dashboard.client.metadata">

    <ui:style src="../EditSocatMetadata.css"/>

    <g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="firstNameValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="middleInitValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="lastNameValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="idTypeValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="idValue"/>
        </g:FlowPanel>
        <my:LabeledTextBox addStyleNames="{style.areapanel}" ui:field="orgValue"/>
        <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="streetsValue"/>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="cityValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="regionValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="zipValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="countryValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="emailValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="phoneValue"/>
        </g:FlowPanel>
    </g:FlowPanel>

</ui:UiBinder>
