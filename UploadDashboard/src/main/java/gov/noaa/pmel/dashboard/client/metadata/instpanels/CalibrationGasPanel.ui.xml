<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:my="urn:import:gov.noaa.pmel.dashboard.client.metadata">

    <ui:style src="../EditSocatMetadata.css"/>

    <g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="typeValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="idValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="frequencyValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="supplierValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="concValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="accuracyValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <g:Button addStyleNames="{style.modbutton}" ui:field="addButton"/>
            <g:Button addStyleNames="{style.modbutton}" ui:field="removeButton"/>
        </g:FlowPanel>
    </g:FlowPanel>

</ui:UiBinder>
