<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="LabeledTextBox.css"/>

    <g:FlowPanel>
        <g:Label addStyleNames="{style.prefixlabel}" ui:field="prefixLabel"/>
        <g:TextBox addStyleNames="{style.valuebox}" ui:field="valueBox"/>
        <g:Label addStyleNames="{style.suffixlabel}" ui:field="suffixLabel"/>
    </g:FlowPanel>

</ui:UiBinder>