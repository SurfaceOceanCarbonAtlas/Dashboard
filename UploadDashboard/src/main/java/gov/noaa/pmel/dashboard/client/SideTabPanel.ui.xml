<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="SideTabPanel.css"/>

    <g:DockLayoutPanel unit="EM">
        <g:west size="7.5">
            <g:FlowPanel addStyleNames="{style.sidepanel}">
                <g:Button addStyleNames="{style.modbutton}" ui:field="addButton"/>
                <g:Button addStyleNames="{style.modbutton}" ui:field="removeButton"/>
                <g:ListBox addStyleNames="{style.valslist}" visibleItemCount="10" ui:field="valsList"/>
                <g:FlowPanel addStyleNames="{style.stashpanel}" visible="false" ui:field="stashPanel"/>
            </g:FlowPanel>
        </g:west>
        <g:center>
            <g:ScrollPanel addStyleNames="{style.mainscroll}" ui:field="mainPanel"/>
        </g:center>
    </g:DockLayoutPanel>

</ui:UiBinder>
