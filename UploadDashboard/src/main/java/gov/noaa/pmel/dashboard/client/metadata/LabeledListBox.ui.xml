<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:style src="LabeledText.css"/>

    <g:FlowPanel>
        <g:HTML addStyleNames="{style.boxprefix}" ui:field="prefixHtml"/>
        <g:ListBox addStyleNames="{style.boxvalue}" visibleItemCount="1" ui:field="valueBox"/>
        <g:Label addStyleNames="{style.suffix}" ui:field="suffixLabel"/>
    </g:FlowPanel>

</ui:UiBinder>
