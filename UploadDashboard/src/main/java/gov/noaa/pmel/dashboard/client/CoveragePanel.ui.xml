<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my='urn:import:gov.noaa.pmel.dashboard.client'>

    <ui:style src="EditSocatMetadata.css"/>

    <g:FlowPanel>
        <g:Label addStyleNames="{style.header}" ui:field="headerLabel"/>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="spacialValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="southLatValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="northLatValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="westLonValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="eastLonValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="startDateValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="endDateValue"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.rowpanel}">
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="earlyDateValue"/>
            <my:LabeledTextBox addStyleNames="{style.cellpanel}" ui:field="lateDateValue"/>
        </g:FlowPanel>
        <my:LabeledTextArea addStyleNames="{style.areapanel}" ui:field="regionsValue"/>
    </g:FlowPanel>

</ui:UiBinder>
