<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my='urn:import:gov.noaa.pmel.dashboard.client'>

    <ui:style src="Coverage.css"/>

    <g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.textpanel}">
            <my:LabeledTextBox addStyleNames="{style.spacialbox}" ui:field="spacialValueBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.textpanel}">
            <my:LabeledTextBox addStyleNames="{style.southlatbox}" ui:field="southLatValueBox"/>
            <my:LabeledTextBox addStyleNames="{style.northlatbox}" ui:field="northLatValueBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.textpanel}">
            <my:LabeledTextBox addStyleNames="{style.westlonbox}" ui:field="westLonValueBox"/>
            <my:LabeledTextBox addStyleNames="{style.eastlonbox}" ui:field="eastLonValueBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.textpanel}">
            <my:LabeledTextBox addStyleNames="{style.startdatebox}" ui:field="startDateBox"/>
            <my:LabeledTextBox addStyleNames="{style.enddatebox}" ui:field="endDateBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.textpanel}">
            <my:LabeledTextBox addStyleNames="{style.earlydatebox}" ui:field="earlyDateBox"/>
            <my:LabeledTextBox addStyleNames="{style.latedatebox}" ui:field="lateDateBox"/>
        </g:FlowPanel>
        <g:CaptionPanel addStyleNames="{style.regionspanel}" ui:field="regionsPanel">
            <g:TextArea addStyleNames="{style.regionsbox}" ui:field="regionsBox"/>
        </g:CaptionPanel>
    </g:FlowPanel>

</ui:UiBinder>
