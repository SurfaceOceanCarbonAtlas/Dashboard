<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="Coverage.css"/>

    <g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.spatialpanel}">
            <g:Label addStyleNames="{style.spatiallabel}" ui:field="spacialLabel"/>
            <g:TextBox addStyleNames="{style.spatialbox}" ui:field="spacialBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.latpanel}">
            <g:Label addStyleNames="{style.southlatlabel}" ui:field="southLatLabel"/>
            <g:TextBox addStyleNames="{style.southlatvaluebox}" ui:field="southLatValueBox"/>
            <g:TextBox addStyleNames="{style.southlatunitbox}" ui:field="southLatUnitBox"/>
            <g:Label addStyleNames="{style.northlatlabel}" ui:field="northLatLabel"/>
            <g:TextBox addStyleNames="{style.northlatvaluebox}" ui:field="northLatValueBox"/>
            <g:TextBox addStyleNames="{style.northlatunitbox}" ui:field="northLatUnitBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.lonpanel}">
            <g:Label addStyleNames="{style.westlonlabel}" ui:field="westLonLabel"/>
            <g:TextBox addStyleNames="{style.westlonvaluebox}" ui:field="westLonValueBox"/>
            <g:TextBox addStyleNames="{style.westlonunitbox}" ui:field="westLonUnitBox"/>
            <g:Label addStyleNames="{style.eastlonlabel}" ui:field="eastLonLabel"/>
            <g:TextBox addStyleNames="{style.eastlonvaluebox}" ui:field="eastLonValueBox"/>
            <g:TextBox addStyleNames="{style.eastlonunitbox}" ui:field="eastLonUnitBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.datepanel}">
            <g:Label addStyleNames="{style.earlydatelabel}" ui:field="earlyDateLabel"/>
            <g:TextBox addStyleNames="{style.earlydatebox}" ui:field="earlyDateBox"/>
            <g:Label addStyleNames="{style.latedatelabel}" ui:field="lateDateLabel"/>
            <g:TextBox addStyleNames="{style.latedatebox}" ui:field="lateDateBox"/>
        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.regionspanel}">
            <g:Label addStyleNames="{style.regionslabel}" ui:field="regionsLabel"/>
            <g:TextArea addStyleNames="{style.regionsbox}" ui:field="regionsBox"/>
        </g:FlowPanel>
    </g:FlowPanel>

</ui:UiBinder>
