<datasets>
  <socat_v2_decadal name="SOCAT v2 decadal 1x1 gridded"
                    url="/home/data/socat/v2/SOCATv2_tracks_gridded_decadal.nc"
                    doc="doc/SOCAT_gridded_decadal.html">
    <properties>
      <ferret>
        <contour_style>raster</contour_style>
        <timelab_res>year</timelab_res>
        <palette>rainbow</palette>
      </ferret>
    </properties>
    <variables>
      <COUNT_NCRUISE_DECADE-socat_v2_decadal name="Number of cruises" units="count" url="#COUNT_NCRUISE_DECADE">
        <link match="/lasdata/grids/grid-XLON-YLAT-TDECADE-socat_v2_decadal" />
        <properties>
          <ferret>
            <fill_levels>40v,0min</fill_levels>
            <palette>lightgray_bottom</palette>
            <contour_style>raster</contour_style>
            <seasonal_transform>SUM</seasonal_transform>
          </ferret>
        </properties>
      </COUNT_NCRUISE_DECADE-socat_v2_decadal>
      <COUNT_NOBS_DECADE-socat_v2_decadal name="Number of obs" units="count" url="#COUNT_NOBS_DECADE">
        <link match="/lasdata/grids/grid-XLON-YLAT-TDECADE-socat_v2_decadal" />
        <properties>
          <ferret>
            <fill_levels>40v,0min</fill_levels>
            <palette>lightgray_bottom</palette>
            <contour_style>raster</contour_style>
            <seasonal_transform>SUM</seasonal_transform>
          </ferret>
        </properties>
      </COUNT_NOBS_DECADE-socat_v2_decadal>
      <FCO2_AVE_UNWTD_DECADE-socat_v2_decadal name="fCO2 mean - unweighted all obs" units="uatm" url="#FCO2_AVE_UNWTD_DECADE">
        <link match="/lasdata/grids/grid-XLON-YLAT-TDECADE-socat_v2_decadal" />
      </FCO2_AVE_UNWTD_DECADE-socat_v2_decadal>
      <FCO2_AVE_WEIGHTED_DECADE-socat_v2_decadal name="fCO2 mean - per cruise weighted" units="uatm" url="#FCO2_AVE_WEIGHTED_DECADE">
        <link match="/lasdata/grids/grid-XLON-YLAT-TDECADE-socat_v2_decadal" />
      </FCO2_AVE_WEIGHTED_DECADE-socat_v2_decadal>
      <FCO2_MAX_UNWTD_DECADE-socat_v2_decadal name="max fCO2" units="uatm" url="#FCO2_MAX_UNWTD_DECADE">
        <link match="/lasdata/grids/grid-XLON-YLAT-TDECADE-socat_v2_decadal" />
        <properties>
          <ferret>
            <seasonal_transform>MAX</seasonal_transform>
          </ferret>
        </properties>
      </FCO2_MAX_UNWTD_DECADE-socat_v2_decadal>
      <FCO2_MIN_UNWTD_DECADE-socat_v2_decadal name="min fCO2" units="uatm" url="#FCO2_MIN_UNWTD_DECADE">
        <link match="/lasdata/grids/grid-XLON-YLAT-TDECADE-socat_v2_decadal" />
        <properties>
          <ferret>
            <seasonal_transform>MIN</seasonal_transform>
          </ferret>
        </properties>
      </FCO2_MIN_UNWTD_DECADE-socat_v2_decadal>
    </variables>
  </socat_v2_decadal>
</datasets>
<grids>
  <grid-XLON-YLAT-TDECADE-socat_v2_decadal>
    <link match="/lasdata/axes/XLON-x-socat_v2_decadal" />
    <link match="/lasdata/axes/YLAT-y-socat_v2_decadal" />
    <link match="/lasdata/axes/TDECADE-t-socat_v2_decadal" />
  </grid-XLON-YLAT-TDECADE-socat_v2_decadal>
</grids>
<axes>
  <XLON-x-socat_v2_decadal type="x" units="degrees_east">
    <arange start="-179.5" size="360" step="1" />
  </XLON-x-socat_v2_decadal>
  <YLAT-y-socat_v2_decadal type="y" units="degrees_north">
    <arange start="-89.5" size="180" step="1" />
  </YLAT-y-socat_v2_decadal>
  <TDECADE-t-socat_v2_decadal type="t" units="year">
    <v>1-JAN-1975</v>
    <v>1-JAN-1985</v>
    <v>1-JAN-1995</v>
    <v>1-JAN-2005</v>
  </TDECADE-t-socat_v2_decadal>
</axes>
