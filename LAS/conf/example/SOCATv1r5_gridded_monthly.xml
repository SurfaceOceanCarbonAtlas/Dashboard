<datasets>
  <socat_v1r5_monthly name="SOCAT v1.5 monthly 1x1 gridded"
                      url="/home/data/socat/v1r5/SOCATv1r5_tracks_gridded_monthly.nc"
                      doc="doc/SOCAT_gridded_monthly.html">
    <properties>
      <ferret>
        <contour_style>raster</contour_style>
        <timelab_res>month</timelab_res>
        <palette>rainbow</palette>
      </ferret>
    </properties>
    <variables>
      <COUNT_NCRUISE-socat_v1r5_monthly name="Number of cruises" units="count" url="#COUNT_NCRUISE">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
        <properties>
          <ferret>
            <fill_levels>40v,0min</fill_levels>
            <palette>lightgray_bottom</palette>
            <contour_style>raster</contour_style>
            <seasonal_transform>SUM</seasonal_transform>
          </ferret>
        </properties>
      </COUNT_NCRUISE-socat_v1r5_monthly>
      <COUNT_NOBS-socat_v1r5_monthly name="Number of obs" units="count" url="#COUNT_NOBS">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
        <properties>
          <ferret>
            <fill_levels>40v,0min</fill_levels>
            <palette>lightgray_bottom</palette>
            <contour_style>raster</contour_style>
            <seasonal_transform>SUM</seasonal_transform>
          </ferret>
        </properties>
      </COUNT_NOBS-socat_v1r5_monthly>
      <FCO2_AVE_UNWTD-socat_v1r5_monthly name="fCO2 mean - unweighted all obs" units="uatm" url="#FCO2_AVE_UNWTD">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
      </FCO2_AVE_UNWTD-socat_v1r5_monthly>
      <FCO2_AVE_WEIGHTED-socat_v1r5_monthly name="fCO2 mean - per cruise weighted" units="uatm" url="#FCO2_AVE_WEIGHTED">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
      </FCO2_AVE_WEIGHTED-socat_v1r5_monthly>
      <FCO2_MAX-socat_v1r5_monthly name="max fCO2" units="uatm" url="#FCO2_MAX">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
        <properties>
          <ferret>
            <seasonal_transform>MAX</seasonal_transform>
          </ferret>
        </properties>
      </FCO2_MAX-socat_v1r5_monthly>
      <FCO2_MIN-socat_v1r5_monthly name="min fCO2" units="uatm" url="#FCO2_MIN">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
        <properties>
          <ferret>
            <seasonal_transform>MIN</seasonal_transform>
          </ferret>
        </properties>
      </FCO2_MIN-socat_v1r5_monthly>
      <FCO2_STD_UNWTD-socat_v1r5_monthly name="fCO2 std dev - unweighted all obs" units="uatm" url="#FCO2_STD_UNWTD">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
        <properties>
          <ferret>
            <fill_levels>40v,0min</fill_levels>
          </ferret>
        </properties>
      </FCO2_STD_UNWTD-socat_v1r5_monthly>
      <FCO2_STD_WEIGHTED-socat_v1r5_monthly name="fCO2 std dev - per cruise weighted" units="uatm" url="#FCO2_STD_WEIGHTED">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
        <properties>
          <ferret>
            <fill_levels>40v,0min</fill_levels>
          </ferret>
        </properties>
      </FCO2_STD_WEIGHTED-socat_v1r5_monthly>
      <LAT_OFFSET_UNWTD-socat_v1r5_monthly name="latitude average offset from cell center" units="Deg N" url="#LAT_OFFSET_UNWTD">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
      </LAT_OFFSET_UNWTD-socat_v1r5_monthly>
      <LON_OFFSET_UNWTD-socat_v1r5_monthly name="longitude average offset from cell center" units="Deg E" url="#LON_OFFSET_UNWTD">
        <link match="/lasdata/grids/grid-XLON-YLAT-TMNTH-socat_v1r5_monthly" />
      </LON_OFFSET_UNWTD-socat_v1r5_monthly>
    </variables>
  </socat_v1r5_monthly>
</datasets>
<grids>
  <grid-XLON-YLAT-TMNTH-socat_v1r5_monthly>
    <link match="/lasdata/axes/XLON-x-socat_v1r5_monthly" />
    <link match="/lasdata/axes/YLAT-y-socat_v1r5_monthly" />
    <link match="/lasdata/axes/TMNTH-t-socat_v1r5_monthly" />
  </grid-XLON-YLAT-TMNTH-socat_v1r5_monthly>
</grids>
<axes>
  <XLON-x-socat_v1r5_monthly type="x" units="DEGREES_EAST">
    <arange start="-179.5" size="360" step="1" />
  </XLON-x-socat_v1r5_monthly>
  <YLAT-y-socat_v1r5_monthly type="y" units="DEGREES_NORTH">
    <arange start="-89.5" size="180" step="1" />
  </YLAT-y-socat_v1r5_monthly>
  <TMNTH-t-socat_v1r5_monthly type="t" units="month">
    <arange start="1970-01-16 12:00:00" size="456" step="1" />
  </TMNTH-t-socat_v1r5_monthly>
</axes>
