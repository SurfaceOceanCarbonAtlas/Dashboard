<grids>
  <grid-XLON-YLAT-TYEAR-socat_v6_yearly>
    <link match="/lasdata/axes/XLON-x-socat_v6_yearly" />
    <link match="/lasdata/axes/YLAT-y-socat_v6_yearly" />
    <link match="/lasdata/axes/TYEAR-t-socat_v6_yearly" />
  </grid-XLON-YLAT-TYEAR-socat_v6_yearly>
</grids>
<axes>
  <XLON-x-socat_v6_yearly type="x" units="degrees_east">
    <arange start="-179.5" size="360" step="1" />
  </XLON-x-socat_v6_yearly>
  <YLAT-y-socat_v6_yearly type="y" units="degrees_north">
    <arange start="-89.5" size="180" step="1" />
  </YLAT-y-socat_v6_yearly>
  <TYEAR-t-socat_v6_yearly type="t" units="year">
    <arange start="1970-01-15" size="48" step="1" />
  </TYEAR-t-socat_v6_yearly>
</axes>
