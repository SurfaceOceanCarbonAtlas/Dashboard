<?xml version="1.0" encoding="UTF-8"?>

<column_info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="SanityChecker_ColumnInfo" xmlns:SanityChecker="SanityChecker_ColumnInfo" xsi:schemaLocation="SanityChecker_ColumnInfo SanityChecker_ColumnInfo.xsd">
  <socat_column name="latitude">
    <input_column index="1">lat</input_column>
		<input_units>dddmm.mmm</input_units>
	</socat_column>
<socat_column name="longitude">
		<input_column index="2">lon</input_column>
		<input_units>dddmm.mmm</input_units>
		<missing_value>-999</missing_value>
	</socat_column>
  <date_columns>
    <year index="3">date</year>
    <month index="3">date</month>
    <day index="3">date</day>
    <hour index="3">date</hour>
    <minute index="3">date</minute>
    <second index="3">date</second>
  </date_columns>
  </column_info>
